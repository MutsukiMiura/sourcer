{
  "name": "Sourcer",
  "website": "https://benishouga.github.io/sourcer/standalone.html",
  "repository": "https://github.com/benishouga/sourcer",
  "logo": "https://raw.githubusercontent.com/benishouga/sourcer/master/src/statics/favicon.ico",
  "success_url": "/",
  "keywords": [],
  "env": {
    "APP_KEY": {
      "description": "This is the key to use when signing up. It is used to limit the user of the application.",
      "required": false,
      "value": ""
    },
    "TEAM_GAME": {
      "description": "Set it to 'true' when used in group work. It can enter the name of the teammate.",
      "required": true,
      "value": "false"
    },
    "ADMIN_PASSWORD": {
      "description": "Password of 'admin' with special authority.",
      "required": false,
      "value": ""
    },
    "SESSION_SECRET": {
      "description": "Session cookie secret.",
      "required": false,
      "value": ""
    },
    "PUBLISH_GAMES": {
      "description": "Set it to 'true' to show the game to the guest.",
      "required": false,
      "value": "false"
    },
    "ENV_MESSAGE_EN": {
      "description": "Display messages on some screens.",
      "required": false,
      "value": "{ \"topMessage\": \"\" }"
    },
    "ENV_MESSAGE_JA": {
      "description": "Display messages on some screens. (for Japanese)",
      "required": false,
      "value": "{ \"topMessage\": \"\" }"
    },
    "NPM_CONFIG_PRODUCTION": {
      "description": "Do not change.",
      "required": true,
      "value": "false"
    }
  },
  "addons": [
    "mongolab"
  ]
}